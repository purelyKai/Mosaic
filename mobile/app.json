{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mobile.mobile",
      "config": {
        "googleMapsApiKey": "AIzaSyBr9CRNYdqljL6dLtFfN7qAAqAxgpFYTHM"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow Mosaic to use your location",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow Mosaic to use your location",
        "NSLocationAlwaysUsageDescription": "Allow Mosaic to use your location",
        "UIBackgroundModes": [
          "location",
          "location"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.mobile.mobile",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBr9CRNYdqljL6dLtFfN7qAAqAxgpFYTHM"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/white.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Mosaic to use your location"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.968440012903-g9t5jtr2jc2khfuu3mtaa0q4bofrqccb"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a2dcfcf8-ad74-4830-9e3d-fcea04efe29c"
      }
    }
  }
}
